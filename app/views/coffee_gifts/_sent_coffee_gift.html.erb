<ul class="no-bullet">
<% coffee_gifts.each do |gift| %>
  <% if gift.redeemed %>
    <li class="coffee_padding">
    <%= link_to "#{gift.cafe.name} (#{gift.name})", '#', class: "coffee-gift redeemed" %>
    </li>
  <% else %>
    <li class="coffee_padding">
    <%= link_to "#{gift.cafe.name} (#{gift.name})", '#', class: "coffee-gift unredeemed" %>
    </li>
  <% end %>
  <br>
<%end%>
</ul>
